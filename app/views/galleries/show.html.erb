<h2> This is a page for the <%= @gallery.name %> gallery</h2> 

<p><%= @gallery.description %></p>

<%= link_to "Add Image", new_gallery_image_path(@gallery) %>

<ul>
  <% @gallery.images.each do |image| %>
    <li><%= image_tag image.url %>
  <%= link_to image.name, gallery_image_path(@gallery,image) %>
  </li> <% end %>

   
