<% @liked_images.each do |liked_image| %>
  <%= link_to image_tag(liked_image.url),
    gallery_image_path(liked_image.gallery, liked_image) %>
  </a>
  <% if current_user.likes?(liked_image) %>
    <%= button_to "Unlike", image_like_path(liked_image), method: :delete %>
  <% else %>
    <%= button_to "Like", image_like_path(liked_image) %>
  <% end %>
<% end %>
